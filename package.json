{
  "name": "webxash",
  "private": true,
  "version": "0.0.0",
  "description": "WebXash Electron + Vite app",
  "author": "Your Name <you@example.com>",
  "homepage": "https://x8bitrain.github.io/webXash",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron electron/main.js",
    "build": "vue-tsc -b && vite build",
    "electron:build": "npm run build && npx electron-builder --publish=never",
    "preview": "vite preview",
    "deploy": "bun run build && touch 'dist/.nojekyll' && gh-pages -d dist"
  },
  "dependencies": {
    "browser-fs-access": "0.37.0",
    "cs16-client": "^0.0.2",
    "fflate": "0.8.2",
    "gh-pages": "^6.3.0",
    "hlsdk-portable": "^0.0.2",
    "pinia": "3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "3.5.13",
    "xash3d-fwgs": "1.0.1"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.3",
    "@eslint/js": "9.20.0",
    "@rushstack/eslint-patch": "1.10.5",
    "@types/node": "22.15.30",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.4.0",
    "@vue/tsconfig": "0.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.5.1",
    "sass-embedded": "1.81.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.24.1",
    "vite": "latest",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.2.8",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.ark.webxash",
    "productName": "webXash",
    "directories": {
      "buildResources": "electron/resources",
      "output": "dist"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "linux": {
      "target": ["AppImage", "deb"]
    },
    "win": {
      "target": ["nsis"]
    }
  }
}
